CREATE OR REPLACE TABLE `sql-portfolio-project-478121.healthcare_data.patient_correlation` AS
SELECT
  CORR(combined_quality_score, patient_experience_score) AS corr_quality_patient_experience,
  CORR(mortality_score, patient_experience_score)          AS corr_mortality_experience,
  CORR(readmission_score, patient_experience_score)        AS corr_readmission_experience
FROM `sql-portfolio-project-478121.healthcare_data.hospital_quality_scored`;
